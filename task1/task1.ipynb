    "n, m = map(int, input().split())\n",
    "\n",
    "def next_elem(i):\n",
    "    elem = 1 + (i + m - 2) % n\n",
    "    return elem\n",
    "\n",
    "i = 1\n",
    "while True:\n",
    "    print(i, end='')\n",
    "    i = next_elem(i)\n",
    "    if i == 1:\n",
    "        break"
   ]
